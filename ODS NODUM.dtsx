                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select cc.fcomienzo_cont, cc.num_cont , cc.cod_alumno alumno_ndm , cc.activo_fcc activo , cc.cod_articulo , ca.cod_tit2 alumno_ps ,
ca3.cod_dpto , ca3.cod_clasedpto , ca3.cod_tipodpto , ca3.cod_negocio , ca3.cod_carrera_sis
from ct_contratoscamp cc
inner join ct_alumnos ca ON cc.cod_alumno = ca.cod_alumno
inner join (
select dp.cod_dpto , dp.cod_clasedpto , dp.cod_tipodpto , ca2.cod_articulo , ca2.cod_negocio , ca2.cod_carrera_sis
from ct_dptos dp , ct_articulos ca2
where dp.cod_dpto = ca2.cod_dpto and ca2.formulario = 'MCampServicios' and left(dp.cod_dpto,1) in ('1','2','3','4','5','6','7','8','9')
) ca3 ON ca3.cod_articulo = cc.cod_articulo
-- where  ca3.cod_carrera_sis = '94' and ca.cod_tit2='27844'</property>
